{
    "version": 2,
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index"
        },
        {
            "handle": "filesystem"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}